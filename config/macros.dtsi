macro_test: macro_test{
compatible = "zmk,behavior-macro";
label = "macro_test";
#binding-cells = <0>;
bindings
  bindings
    = <&macro_tap_time 10>
    , <&kp S &kp H &kp O &kp R &kp T>
    , <&macro_tap_time 500>
    , <&kp L &kp O &kp N &kp G>
    ; 
};
macro_email: macro_email{
compatible = "zmk,behavior-macro";
label = "macro_email";
#binding-cells = <0>;
bindings
= <&kp LS(M)>, <&kp A>, <&kp R>, <&kp K>, <&kp DOT>, <&kp LS(H)>, <&kp A>, <&kp N>, <&kp S>, <&kp E>, <&kp N>,
  <&kp AT_SIGN>, <&kp LS(W)>, <&kp A>, <&kp R>, <&kp N>, <&kp E>, <&kp R>, <&kp LS(M)>, <&kp E>, <&kp D>, <&kp I>, <&kp A>,
  <&kp DOT>, <&kp C>, <&kp O>, <&kp M>, <&kp TAB>;
};