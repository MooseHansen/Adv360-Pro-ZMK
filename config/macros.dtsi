macro_intl: macro_intl{
compatible = "zmk,behavior-macro";
label = "macro_intl";
#binding-cells = <0>;
bindings = <&kp LS(W)>, <&kp E>, <&kp A>, <&kp R>, <&kp G>, <&kp R>, <&kp E>, <&kp A>, <&kp T>;
};
macro_email: macro_email{
compatible = "zmk,behavior-macro";
label = "macro_email";
#binding-cells = <0>;
bindings = <&kp LS(M)>, <&kp A>, <&kp R>, <&kp K>, <&kp DOT>, <&kp LS(H)>;
};
macro_copy: macro_copy{
compatible = "zmk,behavior-macro";
label = "macro_copy";
#binding-cells = <0>;
bindings = <&kp LC(C)>;
};
macro_paste: macro_paste{
compatible = "zmk,behavior-macro";
label = "macro_paste";
#binding-cells = <0>;
bindings = <&kp LC(V)>;
};